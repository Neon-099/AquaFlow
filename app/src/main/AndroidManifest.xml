            android:exported="false" />
        <activity
            android:name=".OrderFormPage"
            android:exported="false" />
        <activity
            android:name=".ProfilePage"
            android:exported="false" />
        <activity
            android:name=".RiderOrderPage"
            android:exported="false" />
        <activity
            android:name=".RiderMessagePage"
